<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Version>24.1.7.1</Version>
	  <Description>Wrapper around Html2Markdown library</Description>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Html2Markdown" Version="6.2.0.3" />
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0">
    </PackageReference>
    <PackageReference Include="HtmlAgilityPack" Version="1.11.57" />
    <PackageReference Include="SunamoHtml" Version="24.1.5.1" />
    <PackageReference Include="SunamoStringReplace" Version="24.1.5.1" />
    <PackageReference Include="TextCopy" Version="6.2.1" />
  </ItemGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>$(DefineConstants);ASYNC</DefineConstants>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>$(DefineConstants);ASYNC</DefineConstants>
  </PropertyGroup>
</Project>